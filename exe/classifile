#!/usr/bin/env ruby
# frozen_string_literal: true

require "optparse"
opt = OptionParser.new
opt.on("-f", "--from PATH", "target path") { |v| @from = v }
opt.on("-t", "--to PATH", "output path") { |v| @to = v }
opt.on("-h", "--help", "show this help") do
  puts opt
  exit
end
opt.parse!(ARGV)
@dsl =  ARGV[0] if ARGV.length.positive?

require "classifile"
exe =  Classifile::Execute.new
exe.test @dsl ,@from ,@to